{
  "name": "geotrust-match",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@creit.tech/stellar-wallets-kit": "^1.9.5",
    "@mapbox/mapbox-gl-draw": "^1.5.1",
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "@stellar/freighter-api": "^1.7.0",
    "@stellar/stellar-sdk": "^14.5.0",
    "buffer": "^6.0.3",
    "mapbox-gl": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "serve": "^14.2.1"
  },
  "scripts": {
    "start": "set PORT=3366 && react-scripts start",
    "build": "react-scripts build && node scripts/create-server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 8080",
    "azure-start": "cd server && npm start",
    "start:prod": "cd server && npm start",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "dev": "concurrently \"npm run server:dev\" \"npm start\"",
    "install-all": "npm install && cd server && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/mapbox__mapbox-gl-draw": "^1.4.9",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^8.2.2"
  },
  "proxy": "http://localhost:8080"
}
